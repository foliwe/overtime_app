 <%recent_audit_items.each do |recent_items|%>
			<div class="homepage-block col-md-3">
				<h4><%= recent_items.user.full_name %></h4>

				<p>
				   <span class="pending-details">Week starting:</span> <%=recent_items.start_date %>
				</p>
				<p>
				   <span class="pending-details">Confirmed at:</span> <%=recent_items.end_date || status_label('pending')%>
				</p>
				<p>
				   <span class="pending-details">Status:</span> <%=status_label recent_items.status%>
				</p>
				
			</div>
		<%end %>